{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "ApiBaseUrl": "https://localhost:7260/",
    "TimeoutSeconds": 100,
    "EndPoints": {
      "DHT": {
        "FetchAccountInfo": "api/dht/fetchaccountinfo"
      }
    }
  },
  "CacheSettings": {
    "CacheType": "Redis",
    "Redis": {
      "Configuration": "localhost:6379",
      "InstanceName": "KHS"
    },
    "Azure": {
      "ConnectionString": "azure-connection-string"
    },
    "Aws": {
      "Endpoint": "aws-elasticache-endpoint:6379",
      "AccessKey": "access-key",
      "SecretKey": "secret-key",
      "Region": "region"
    }
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "Verify"
  },
  "AllowedHosts": "*"
}
